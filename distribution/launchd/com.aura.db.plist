<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.aura.db</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/aura-server</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/var/log/aura.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/aura_err.log</string>
    <key>UserName</key>
    <string>_aura</string>
    <key>WorkingDirectory</key>
    <string>/var/lib/aura</string>
</dict>
</plist>